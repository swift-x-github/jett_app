<%= javascript_pack_tag 'sticky-kit' %>
<%= javascript_pack_tag 'rangeSlider' %>
<%= javascript_pack_tag 'owl_carousel' %>
<%= javascript_pack_tag 'chosen' %>
<%= javascript_pack_tag 'magnific-popup' %>
<%= javascript_pack_tag 'tooltips' %>
<%= javascript_pack_tag 'typed' %>
<%= javascript_pack_tag 'mmenu' %>
<%= javascript_pack_tag 'custom_jquery' %>


  </body>
</html>
